{
    "manifest_version": 3,
    "name": "LLMプロンプトテンプレート",
    "version": "1.0",
    "description": "LLMプロンプトのテンプレートを挿入するツールです。",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "import.html"
    }
  }
  